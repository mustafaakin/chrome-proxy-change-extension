{
	"name": "Set a HTTP Proxy",
	"version": "1.0",
	"description": "Sets an HTTP Proxy easily without extensive rights",
	"manifest_version": 2,
	"permissions": [
		"proxy"
	],
	"browser_action": {
		"default_popup": "popup.html",
		  "default_icon": {
		  "192": "192.png"
	  }
	},
	"icons": {
		"192": "192.png"
	}
}